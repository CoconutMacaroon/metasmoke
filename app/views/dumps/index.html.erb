<h1>Dumps</h1>

<table class="table">
  <thead>
    <tr>
      <th>File</th>
    </tr>
  </thead>

  <tbody>
    <% @dumps.each do |dump| %>
      <tr>
        <td><%= link_to dump.file_file_name, dump %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h3>Redis dumps</h3>

<table class="table">
  <thead>
    <tr>
      <th>File</th>
    </tr>
  </thead>
  <tbody>
    <% @redis_dumps&.each do |dump| %>
      <tr>
        <td><%= link_to dump[:name], dump[:url] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
