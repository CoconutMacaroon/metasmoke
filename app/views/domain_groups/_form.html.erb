<%= form_for @group, url: url do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :regex %>
    <%= f.text_field :regex, class: 'form-control' %>
  </div>

  <%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>